<defines>
  <!-- Special cases for TeX-like accent macros -->
  <def m='"a'><d>ä</d></def> <!-- &auml; -->
  <def m='"e'><d>ë</d></def> <!-- &euml; -->
  <def m='"i'><d>ï</d></def> <!-- &iuml; -->
  <!-- ... -->

  <def m="`a"><d>à</d></def> <!-- &agrave; -->
  <def m="`e"><d>è</d></def> <!-- &egrave; -->
  <def m="`i"><d>ì</d></def> <!-- &igrave; -->
  <!-- ... -->

  <def m="'a"><d>á</d></def> <!-- &aacute; -->
  <def m="'e"><d>é</d></def> <!-- &eacute; -->
  <def m="'i"><d>í</d></def> <!-- &iacute; -->
  <!-- ... -->
  
  <def m="o"><d>&oslash;</d></def> <!-- ø -->

